#include<stdio.h>
void main()
{
	int i,n,j,c=0;
	printf("Enter array size:");
	printf("%d",&n);
	int a[n];
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			if(a[i]==a[j] && i!=j)
			{
				break;
			}
		}
		if(j==n)
		{
			printf("\n Non repeating element is %d",a[i]);
		}
	}
}
