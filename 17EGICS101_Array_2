#include<stdio.h>
int main()
{
	int a[100], n, sum1=0, sum2=0,i, x;
	scanf("%d",&n);
	if (n%2==0)
	{
		for (i=0;i<n;i++)
		{
			scanf("%d", &a[i]);
		}
			x = n/2;
		for (i=0;i<x;i++)
		{
			sum1 = sum1 + a[i];
		}
		for (i=x;i<n;i++)
		{
			sum2 = sum2 + a[i];
		}
		if(sum1<sum2)
		{
			printf("%d", sum1);
		}
	else
	{
		printf("%d", sum2);
	}
	}
	else
	{
		printf("Please Enter an ever No.");
	}

	return(0);
}
