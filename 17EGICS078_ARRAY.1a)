#include<stdio.h>
void main()
{
	int a[100],f[10]={0},n,i,digit,max,no;
	printf("Enter size of array: ");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Enter %d element (which is a single digit No.):\n",i+1);
		scanf("%d",&a[i]);		
	}
	for(i=0;i<n;i++)
	{
		f[a[i]]++;
	}
	max = f[0];
	for(i=1;i<10;i++)
	{
   		if (f[i] > max)
   		{
        	max = f[i];
        	no = i;
		}
	}
	printf("\n");
	printf("Maximum frequency is of No. %d", no);
}
