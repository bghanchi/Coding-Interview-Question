#include<stdio.h>
void main()
{
  int n;
  printf("enter size of array");
  scanf("%d",&n);
  int a[n],i,j,f=0;
  printf("enter array elements");
  for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);
   }
  for(i=0;i<n;i++)
  {
     for(j=i+1;j<n;j++)
     {        
        if(a[i]>=a[j])
        {  
         f=1;
        }
       else
       {
         f=0;
         break;
       }
     }
    if(f==1)
     {
       printf("leaders=%d\t",a[i]);
     }
  }
}
