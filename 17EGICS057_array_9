#include<stdio.h>
void main()
{
	int n,check;
	printf("enter size of number of elements");
	scanf("%d",&n);
	int a[n],prod[n],i,j;
	printf("enter array elements");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	int pro=1;
	for(i=0;i<n;i++)
	{
		for(j=0;j<n;j++)
		{
			check=i-j;
			if(check!=0)
			{
				pro=pro*a[j];
			}
		}
		prod[i]=pro;
		pro=1;
	}
	for(i=0;i<n;i++)
	{
		printf("%d\n",prod[i]);
	}
}
