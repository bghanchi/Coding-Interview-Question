#include<stdio.h>
void main()
{
	int n,i,t;
	printf("enter size of array\n");
	scanf("%d",&n);
	int a[n];
	printf("enter array elements\n");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter target value\n");
	scanf("%d",&t);
	for(i=0;i<n;i++)
	{
		if(a[i]==t)
		{
			printf("index number=%d",i);
		}
		else if(t>a[i] && t<a[i+1])
		{
			printf("enter at index = %d",i+1);
		}
		else if(a[i]>t)
		{
			printf("enter at index=%d",i);
			break;
		}
	}
}
